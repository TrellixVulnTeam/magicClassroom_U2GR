<?xml version="1.0" encoding="UTF-8"?>
<viewmodel:View
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:viewmodel="com.magicleap.uidesigner.rcp.properties.viewmodel"
    xsi:schemaLocation="com.magicleap.uidesigner.rcp.properties.viewmodel ../../com.magicleap.common.properties.model/model/viewmodel.ecore"
    viewId="vdf.headtracking"
    typeId="HeadTracking"
    inherit="false">
  <children
      xsi:type="viewmodel:VerticalLayout"
      viewId="vdf.headtracking.group">
    <children
        xsi:type="viewmodel:Group"
        viewId="vdf.headtracking.transform"
        label="Transform">
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.headtracking.transform.position"
          id="position"/>
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.headtracking.transform.orientation"
          id="orientation"/>
    </children>
    <children
        xsi:type="viewmodel:Group"
        viewId="vdf.headtracking.state"
        label="State">
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.headtracking.state.confidence"
          id="confidence"
          labelAlignment="Top"/>
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.headtracking.state.error"
          id="error"/>
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.headtracking.state.mode"
          id="mode"/>
    </children>
  </children>
</viewmodel:View>
