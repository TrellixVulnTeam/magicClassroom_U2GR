<?xml version="1.0" encoding="UTF-8"?>
<viewmodel:View
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:viewmodel="com.magicleap.uidesigner.rcp.properties.viewmodel"
    xsi:schemaLocation="com.magicleap.uidesigner.rcp.properties.viewmodel ../../com.magicleap.common.properties.model/model/viewmodel.ecore"
    viewId="vdf.sceneview"
    typeId="SceneView">
  <children
      xsi:type="viewmodel:Group"
      viewId="empty_room_model"
      label="Empty Room Model">
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.SplashModelPosition"
        id="VirtualRoomServerSparseDevice.SplashModelPosition"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.SplashModelRotation"
        id="VirtualRoomServerSparseDevice.SplashModelRotation"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.SplashModelScale"
        id="VirtualRoomServerSparseDevice.SplashModelScale"/>
  </children>
  <children
      xsi:type="viewmodel:Group"
      viewId="nodes"
      label="Nodes">
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.MoveSnapSize"
        id="VirtualRoomServerSparseDevice.MoveSnapSize"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.RotateSnapAngle"
        id="VirtualRoomServerSparseDevice.RotateSnapAngle"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.ScaleSnapSize"
        id="VirtualRoomServerSparseDevice.ScaleSnapSize"/>
  </children>
  <children
      xsi:type="viewmodel:Group"
      viewId="manipulation"
      label="Manipulation">
    <children
        xsi:type="viewmodel:Group"
        viewId="sceneview.empty_room_mode.model_transform"
        label="Starting Transform">
      <children
          xsi:type="viewmodel:Control"
          viewId="sceneview.SceneViewCameraPosition"
          id="VirtualRoomServerSparseDevice.SceneViewCameraPosition"/>
      <children
          xsi:type="viewmodel:Control"
          viewId="sceneview.SceneViewCameraRotation"
          id="VirtualRoomServerSparseDevice.SceneViewCameraRotation"/>
    </children>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.MovementInitialVelocity"
        id="VirtualRoomServerSparseDevice.MovementInitialVelocity"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.MovementAcceleration"
        id="VirtualRoomServerSparseDevice.MovementAcceleration"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.RotationSensitivity"
        id="VirtualRoomServerSparseDevice.RotationSensitivity"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.PanningSensitivityX"
        id="VirtualRoomServerSparseDevice.PanningSensitivityX"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="sceneview.PanningSensitivityY"
        id="VirtualRoomServerSparseDevice.PanningSensitivityY"/>
  </children>
</viewmodel:View>
