<?xml version="1.0" encoding="UTF-8"?>
<viewmodel:View
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:viewmodel="com.magicleap.uidesigner.rcp.properties.viewmodel"
    xsi:schemaLocation="com.magicleap.uidesigner.rcp.properties.viewmodel ../../com.magicleap.common.properties.model/model/viewmodel.ecore"
    viewId="vdf.rendering"
    typeId="Rendering">
  <children
      xsi:type="viewmodel:Group"
      viewId="rendering.general"
      label="General">
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.GlobalAmbientColor"
        id="VirtualRoomServerSparseDevice.GlobalAmbientColor"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.CompositorRoomDarknessBias"
        id="VirtualRoomServerSparseDevice.CompositorRoomDarknessBias"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.RedrawOnDemand"
        id="VirtualRoomServerSparseDevice.RedrawOnDemand"
        labelAlignment="None">
      <style>right-aligned</style>
    </children>
  </children>
  <children
      xsi:type="viewmodel:Group"
      viewId="rendering.scene"
      label="Scene View">
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.SceneViewClearColor"
        id="VirtualRoomServerSparseDevice.SceneViewClearColor"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.ViewGridColor"
        id="VirtualRoomServerSparseDevice.ViewGridColor"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.ViewGridExpansion"
        id="VirtualRoomServerSparseDevice.ViewGridExpansion"
        labelAlignment="None">
      <style>right-aligned</style>
    </children>
  </children>
  <children
      xsi:type="viewmodel:Group"
      viewId="rendering.device"
      label="Device View">
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.CompositorMode"
        id="VirtualRoomServerSparseDevice.CompositorMode"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.CompositorUserContentBrightnessBias"
        id="VirtualRoomServerSparseDevice.CompositorUserContentBrightnessBias"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.CompositorUserContentGammaCorrection"
        id="VirtualRoomServerSparseDevice.CompositorUserContentGammaCorrection"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.GraphicsResolutionScale"
        id="MockML1GraphicsViewSparseDevice.GraphicsResolutionScale"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.GraphicsResolutionScaleML1"
        id="ML1GraphicsViewSparseDevice.GraphicsResolutionScale"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.GraphicsCodecTypeML1"
        id="ML1GraphicsViewSparseDevice.GraphicsCodecType"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.ContractFrameTimeoutMs"
        id="MockML1GraphicsViewSparseDevice.ContractFrameTimeoutMs"/>
    <children
        xsi:type="viewmodel:Control"
        viewId="rendering.ContractFrameTimeoutMsML1"
        id="ML1GraphicsViewSparseDevice.ContractFrameTimeoutMs"/>
  </children>
</viewmodel:View>
